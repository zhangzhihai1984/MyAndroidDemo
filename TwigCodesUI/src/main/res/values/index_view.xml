<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndexView">
        <attr name="debug" />
        <attr name="idleColor" format="reference|color" />
        <attr name="indexedColor" format="reference|color" />
        <attr name="textSize" format="dimension" />
        <attr name="debounceTime" format="integer" />
        <attr name="OffsetDirection" format="enum">
            <enum name="left" value="-1" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>