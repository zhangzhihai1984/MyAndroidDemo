<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndexView">
        <attr name="idleColor" format="reference|color" />
        <attr name="indexedColor" format="reference|color" />
        <attr name="textSize" format="dimension" />
        <attr name="debounceTime" format="integer" />
        <attr name="debug" format="boolean" />
        <attr name="textOffsetDirection" format="enum">
            <enum name="left" value="true" />
            <enum name="right" value="false" />
        </attr>
    </declare-styleable>
</resources>